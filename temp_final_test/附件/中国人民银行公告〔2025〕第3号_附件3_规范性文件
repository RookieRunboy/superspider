/*页面当前位置*/
$(".SkinObject").eq(0).text("首页");
$(".SkinObject").eq(0).attr("href","/rmyh/index.html");	
 /*设为主页*/
function setHome(obj){
 obj.style.behavior='url(#default#homepage)'; 
 obj.setHomePage('http://www.pbc.gov.cn');
 
 return false;
}

/*加入收藏*/
function bookmarkit(){
	switch(getOs()){
		case 1:
			window.external.addFavorite('http://www.pbc.gov.cn','中国人民银行');
				break;
		case 2:
			window.sidebar.addPanel('中国人民银行', 'http://www.pbc.gov.cn', "");
				break;
		case 0:
			alert("加入收藏失败，您使用的浏览器不支持这个功能");
				break;
	}
}

function getOs() { 
	if(navigator.userAgent.indexOf("MSIE")>0)return 1; 
	if(navigator.userAgent.indexOf("Firefox")>0)return 2; 
	return 0; 
} 

 
</script>
